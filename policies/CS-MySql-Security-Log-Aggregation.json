{
  "properties": {
    "displayName": "CS-MySql-Security-Log-Aggregation",
    "policyType": "Custom",
    "mode": "All",
    "metadata": {
      "version": "1.0.1",
      "createdBy": "9e2f9255-3d43-4bdd-86c5-351daef6cbe6",
      "createdOn": "2025-08-20T09:22:00.0103517Z",
      "updatedBy": "9e2f9255-3d43-4bdd-86c5-351daef6cbe6",
      "updatedOn": "2025-08-20T10:08:52.8720184Z"
    },
    "version": "1.0.0",
    "parameters": {},
    "policyRule": {
      "if": {
        "anyOf": [
          {
            "field": "type",
            "equals": "Microsoft.DBforMySQL/servers"
          },
          {
            "field": "type",
            "equals": "Microsoft.DBforMySQL/flexibleServers"
          }
        ]
      },
      "then": {
        "effect": "AuditIfNotExists",
        "details": {
          "type": "Microsoft.Insights/diagnosticSettings",
          "existenceCondition": {
            "field": "name",
            "equals": "CS-mysqldiag"
          }
        }
      }
    },
    "versions": [
      "1.0.0"
    ]
  },
