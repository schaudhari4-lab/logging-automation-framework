{
  "properties": {
    "displayName": "CS-SQLdb-Security-Log-Aggregation",
    "policyType": "Custom",
    "mode": "All",
    "metadata": {
      "version": "1.0.0",
      "createdBy": "9e2f9255-3d43-4bdd-86c5-351daef6cbe6",
      "createdOn": "2025-08-07T10:32:16.7750336Z",
      "updatedBy": "9e2f9255-3d43-4bdd-86c5-351daef6cbe6",
      "updatedOn": "2025-08-20T08:36:45.7189248Z"
    },
    "version": "1.0.0",
    "parameters": {},
    "policyRule": {
      "if": {
        "allOf": [
          {
            "field": "type",
            "equals": "Microsoft.Sql/servers/databases"
          },
          {
            "field": "name",
            "notEquals": "master"
          }
        ]
      },
      "then": {
        "effect": "AuditIfNotExists",
        "details": {
          "type": "Microsoft.Insights/diagnosticSettings",
          "existenceCondition": {
            "field": "name",
            "equals": "CS-sqldiag"
          }
        }
      }
    },
    "versions": [
      "1.0.0"
    ]
  },
